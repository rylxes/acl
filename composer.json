{
    "name": "rylxes/acl",
    "description": "Acl for routes",
    "license": "license",
    "authors": [
        {
            "name": "rylxes",
            "email": "rylxes@gmail.com",
            "homepage": "rylxes.com"
        }
    ],
    "homepage": "https://github.com/rylxes/acl",
    "keywords": ["Laravel", "Acl"],
    "require": {
        "illuminate/support": "~5|~6"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "rylxes\\Acl\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "rylxes\\Acl\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "rylxes\\Acl\\AclServiceProvider"
            ],
            "aliases": {
                "Acl": "rylxes\\Acl\\Facades\\Acl"
            }
        }
    }
}
